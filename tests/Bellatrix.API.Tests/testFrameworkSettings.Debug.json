{
  "troubleshootingSettings": {
    "debugInformationEnabled": "false"
  },
  "apiSettings": {
    "baseUrl": "http://127.0.0.1:60715",
    "clientTimeoutSeconds": "30",
    "maxRetryAttempts": "3",
    "pauseBetweenFailures": "1",
    "timeUnit": "1"
  },
  "azureDevOpsDynamicTestCasesSettings": {
    "isEnabled": "false",
    "url": "https://dev.azure.com/yourCompany",
    "token": "yourToken",
    "organizationName": "organizationName",
    "projectName": "projectName"
  },
  "qTestDynamicTestCasesSettings": {
    "isEnabled": "false",
    "serviceAddress": "https://yourProject.qtestnet.com/",
    "token": "yourAuthenticationToken",
    "userName": "yourUserName",
    "password": "yourPass",
    "projectId": "559370"
  },
  "azureDevOpsBugReportingSettings": {
    "isEnabled": "false",
    "url": "https://dev.azure.com/yourCompany",
    "token": "yourToken",
    "organizationName": "organizationName",
    "projectName": "projectName",
    "defaultPriority": "2",
    "defaultSeverity": "2 - High"
  },
  "jiraBugReportingSettings": {
    "isEnabled": "false",
    "url": "yourServerUrl",
    "token": "authenticationToken",
    "projectName": "projectName",
    "defaultPriority": "Medium"
  },
  "allureReportingSettings": {
    "isEnabled": "false"
  },
  "keyVaultSettings": {
    "keyVaultEndpoint": "keyVaultEndpointUrl",
    "isEnabled": "false"
  },
  "appInsightsSettings": {
    "instrumentationKey": "yourKey",
    "isEnabled": "false"
  },
  "loggingSettings": {
    "isEnabled": true,
    "isConsoleLoggingEnabled": true,
    "isDebugLoggingEnabled": true,
    "isFileLoggingEnabled": false,
    "isReportPortalLoggingEnabled": false,
    "shouldMaskSensitiveInfo": true,
    "outputTemplate": "{Message:lj}{NewLine}{Exception}"
  }
}