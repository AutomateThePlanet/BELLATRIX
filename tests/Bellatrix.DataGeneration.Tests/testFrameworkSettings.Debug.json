  "testValueGenerationSettings": {
    "includeBoundaryValues": true,
    "allowValidEquivalenceClasses": false,
    "allowInvalidEquivalenceClasses": false,
    "inputTypeSettings": {
      "Email": {
        "PrecisionStep": "1",
        "ValidEquivalenceClasses": [ "user@example.com", "contact@domain.net" ],
        "InvalidEquivalenceClasses": [
          "invalid-email",
          "plainaddress",
          "@missingusername.com",
          "missingdomain@",
          "user@.com",
          "user@domain..com"
        ]
      },
      "Phone": {
        "PrecisionStep": "1",
        "ValidEquivalenceClasses": [ "+11234567890", "+442071838750" ],
        "InvalidEquivalenceClasses": [
          "12345",
          "0000000000",
          "abcdefg",
          "+123",
          "+359 888",
          "+359888BADNUM",
          "(123) 456-7890-ext"
        ]
      },
      "Text": {
        "PrecisionStep": "1",
        "ValidEquivalenceClasses": [ "Hello World", "Sample Input" ],
        "InvalidEquivalenceClasses": [
          "",
          " ",
          "\n",
          "\t",
          "!@#$%^&*()",
          "超长文本超长文本超长文本",
          "<script>alert('XSS')</script>",
          "' OR 1=1 --"
        ]
      },
      "Password": {
        "PrecisionStep": "1",
        "ValidEquivalenceClasses": [ "StrongP@ssw0rd1", "Another1#Valid" ],
        "InvalidEquivalenceClasses": [ "12345", "password", "abc", " " ]
      },
      "Boolean": {
        "PrecisionStep": "1",
        "ValidEquivalenceClasses": [ "true", "false" ],
        "InvalidEquivalenceClasses": [ "yes", "no", "1", "0", "", "maybe" ]
      },
      "Integer": {
        "PrecisionStep": "1",
        "ValidEquivalenceClasses": [ 0, 42, -100, 100000 ],
        "InvalidEquivalenceClasses": [ "abc", "", "999999999999999999999", "-999999999999999999999" ]
      },
      "Decimal": {
        "PrecisionStep": "0.01",
        "ValidEquivalenceClasses": [ 10.5, -100.75, 0.00, 9999.99 ],
        "InvalidEquivalenceClasses": [ "NaN", "infinity", "text", "", null ]
      },
      "Percentage": {
        "PrecisionStep": "0.01",
        "ValidEquivalenceClasses": [ 0.0, 50.5, 99.99, 100.0 ],
        "InvalidEquivalenceClasses": [ -1, 101, "text", "", null ]
      },
      "Currency": {
        "PrecisionStep": "0.01",
        "ValidEquivalenceClasses": [ 0.0, 19.99, 100.00, 99999.99 ],
        "InvalidEquivalenceClasses": [ -5, "free", "text", "", null ]
      },
      "Date": {
        "PrecisionStep": "1",
        "PrecisionStepUnit": "Days", // Supported units: "Days", "Months", "Years"
        "ValidEquivalenceClasses": [ "2024-01-01", "1990-12-31", "2025-03-26" ],
        "InvalidEquivalenceClasses": [ "not-a-date", "13/32/2020", "", null ]
      },
      "Time": {
        "PrecisionStep": "15",
        "PrecisionStepUnit": "Minutes", // Supported units: "Seconds", "Minutes", "Hours"
        "ValidEquivalenceClasses": [ "00:00", "12:30", "23:59" ],
        "InvalidEquivalenceClasses": [ "24:00", "99:99", "noon", "", null ]
      },
      "DateTime": {
        "PrecisionStep": "1",
        "PrecisionStepUnit": "Minutes", // Supported units: "Seconds", "Minutes", "Hours", "Days", "Milliseconds"
        "ValidEquivalenceClasses": [ "2024-10-01T10:30:00", "1999-12-31T23:59:59", "2025-03-26T00:00:00" ],
        "InvalidEquivalenceClasses": [ "tomorrow", "32/01/2022 25:00", "not-a-datetime", "", null ]
      },
      "Week": {
        "PrecisionStep": "1",
        "ValidEquivalenceClasses": [ "2025-W01", "2024-W52", "2023-W12" ],
        "InvalidEquivalenceClasses": [ "2025-W60", "2024-W00", "not-a-week", "", null ]
      },
      "Month": {
        "PrecisionStep": "1",
        "ValidEquivalenceClasses": [ "2025-01", "2024-12", "1999-07" ],
        "InvalidEquivalenceClasses": [ "2025-13", "1999-00", "March 2025", "", null ]
      },
      "Address": {
        "PrecisionStep": "10", // number of characters added/removed during boundary offset
        "ValidEquivalenceClasses": [
          "123 Main St, Springfield, IL 62704",
          "456 Elm St, Apt 5B, New York, NY 10001"
        ],
        "InvalidEquivalenceClasses": [
          "",
          "No Address",
          "ZZZ",
          null
        ]
      },
      "GeoCoordinate": {
        "PrecisionStep": "0.01",
        "ValidEquivalenceClasses": [
          "42.6975,23.3242",
          "48.8566,2.3522",
          "-33.8688,151.2093"
        ],
        "InvalidEquivalenceClasses": [
          "NaN,NaN",
          "999,999",
          "text",
          "42.6975",
          "42.6975,",
          "",
          null
        ]
      },
      "Username": {
        "PrecisionStep": "1",
        "ValidEquivalenceClasses": [
          "john_doe",
          "user123",
          "qa_tester",
          "dev_user1"
        ],
        "InvalidEquivalenceClasses": [
          "admin!",
          "user name",
          "root$",
          "",
          null
        ]
      },
      "URL": {
        "PrecisionStep": "5",
        "ValidEquivalenceClasses": [
          "https://www.google.com",
          "http://example.org",
          "https://sub.domain.co.uk"
        ],
        "InvalidEquivalenceClasses": [
          "www.google.com",
          "http:/invalid.com",
          "ftp://wrong.protocol",
          "://missing.scheme.com",
          "",
          null
        ]
      },
      "Color": {
        "ValidEquivalenceClasses": [
          "#FF0000",
          "#00FF00",
          "#0000FF",
          "#123ABC",
          "#000000",
          "#FFFFFF"
        ],
        "InvalidEquivalenceClasses": [
          "FF0000",
          "#GGGGGG",
          "#12345",
          "#1234567",
          "red",
          "",
          null
        ]
      }
    }
  }
}